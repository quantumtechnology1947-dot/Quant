graph TB
    %% Sales Distribution Dashboard Module Architecture Diagram
    
    subgraph "Presentation Layer"
        A[Dashboard.aspx<br/>Sales Distribution Main Dashboard]
        A1[Master Page<br/>MasterPage.master]
        A2[JavaScript Files<br/>loadingNotifier.js]
    end
    
    subgraph "Code-Behind Layer"
        B[Dashboard.aspx.cs<br/>Module_SalesDistribution_Dashboard Class]
        B1[Page_Load Event<br/>Basic initialization]
    end
    
    subgraph "Navigation Structure"
        C[Module Navigation]
        C1[Masters Section<br/>Customer, Products, Categories]
        C2[Transactions Section<br/>Enquiry, Quotation, PO, Work Orders]
        C3[Reports Section<br/>Various business reports]
    end
    
    subgraph "Masters Module"
        D[Master Data Management]
        D1[Customer Master<br/>Customer information management]
        D2[Product Master<br/>Category_Master table]
        D3[Category Management<br/>Work Order categories]
        D4[Sub-Category Management<br/>Work Order sub-categories]
        D5[WO Types<br/>Work Order types]
        D6[WO Release & Dispatch Authority<br/>Authorization management]
    end
    
    subgraph "Transactions Module"
        E[Transaction Processing]
        E1[Customer Enquiry<br/>SD_Cust_Enquiry_Master]
        E2[Quotation Workflow<br/>SD_Cust_Quotation_Master]
        E3[Customer PO<br/>Purchase Order management]
        E4[Work Order<br/>SD_Cust_WorkOrder_Master]
        E5[Work Order Release<br/>SD_Cust_WorkOrder_Release]
        E6[Work Order Dispatch<br/>SD_Cust_WorkOrder_Dispatch]
        E7[Cost Sheet<br/>Cost calculations]
    end
    
    subgraph "Reports Module"
        F[Reporting System]
        F1[Customer Reports<br/>Customer master reports]
        F2[Enquiry Reports<br/>Customer enquiry reports]
        F3[Quotation Reports<br/>Quotation tracking]
        F4[PO Reports<br/>Purchase order reports]
        F5[Work Order Reports<br/>WO tracking and status]
    end
    
    subgraph "Database Layer"
        G[Core Tables]
        G1[SD_Cust_Master<br/>Customer master data]
        G2[SD_Cust_Enquiry_Master<br/>Customer enquiries]
        G3[SD_Cust_Quotation_Master<br/>Quotation headers]
        G4[SD_Cust_Quotation_Details_Temp<br/>Quotation line items]
        G5[SD_Cust_WorkOrder_Master<br/>Work order headers]
        G6[Category_Master<br/>Product categories]
        G7[tblSD_WO_Category<br/>Work order categories]
        G8[tblSD_WO_SubCategory<br/>Work order sub-categories]
        G9[tblFile_Attachment<br/>Document attachments]
    end
    
    subgraph "Business Workflow"
        H[Sales Process Flow]
        H1[Customer Registration<br/>New customer setup]
        H2[Enquiry Creation<br/>Customer enquiry capture]
        H3[Quotation Process<br/>Create → Check → Approve → Authorize]
        H4[PO Processing<br/>Customer purchase order]
        H5[Work Order Creation<br/>Internal work order]
        H6[WO Release & Dispatch<br/>Production and delivery]
    end
    
    subgraph "User Roles & Access"
        I[Role-Based Access]
        I1[Sales Team<br/>Enquiry and quotation management]
        I2[Approval Authority<br/>Quotation approval workflow]
        I3[Production Team<br/>Work order management]
        I4[Dispatch Team<br/>Delivery coordination]
        I5[Management<br/>Reports and oversight]
    end
    
